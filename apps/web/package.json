{
  "name": "@leave/web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --check \"**/*.{ts,tsx,md,json,yml,yaml}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,md,json,yml,yaml}\"",
    "fix": "pnpm format:fix && pnpm lint:fix",
    "fix:file": "sh -c 'prettier --write $1 && eslint --fix $1' --",
    "tc": "tsc --noEmit",
    "tc:file": "sh -c 'tsc --noEmit 2>&1 | grep -A 1 \"$1\"' --",
    "test": "jest",
    "e2e": "playwright test"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.0.18",
    "@leave/types": "*",
    "@tanstack/react-query": "^5.0.0",
    "date-fns": "^2.30.0",
    "next": "14.1.0",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.6",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@leave/tsconfig": "*",
    "@playwright/test": "^1.41.0",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-big-calendar": "^1.8.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.0.1",
    "eslint-config-custom": "*",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-turbo": "^2.4.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-alias": "^1.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "postcss": "^8.0.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.0.0"
  }
}
